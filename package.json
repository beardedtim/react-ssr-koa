{
  "scripts": {
    "check-types": "tsc",
    "fmt": "prettier --write \"src/**/*.{ts,tsx}\"",
    "lint": "eslint ./src",
    "quality-check": "yarn check-types && yarn lint && yarn fmt",
    "build:server": "webpack --config config/webpack.server.js",
    "dev:server": "yarn build:server --watch",
    "build:client": "webpack --config config/webpack.client.js",
    "dev:client": "yarn build:client --watch",
    "build": "yarn build:server && yarn build:client",
    "dev": "rm -rf dist && yarn build:server && concurrently --kill-others -n \"BUILD:SERVER,BUILD:CLIENT,SERVER\" \"yarn dev:server\" \"yarn dev:client\" \"nodemon\""
  },
