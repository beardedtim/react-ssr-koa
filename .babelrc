{
  "presets": [
    [
      "@babel/preset-env",
      {
        "useBuiltIns": "usage",
        "corejs": 3,
        "targets": {
          "node": "current",
          "browsers": [
            "> 0.25%"
          ]
        }
      }
    ],
    ["@babel/typescript"],
    ["@babel/preset-react"]
  ],
  "plugins": [
    ["@babel/proposal-class-properties"],
    ["@babel/proposal-object-rest-spread"],
      [
        "@loadable/babel-plugin"
      ]
  ]
}